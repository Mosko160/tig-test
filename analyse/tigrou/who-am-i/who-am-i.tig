let
function who_am_i(age: int) =
    if age < 3 then print("TIGROS\n")
    else if age < 6 then print("TIGROU\n")
    else print("TIGER\n")
in
  print("Expected : TIGROS\ngot      : ");
  who_am_i(2);
  print("\n");

  print("Expected : TIGROU\ngot      : ");
  who_am_i(3);
  print("\n");

  print("Expected : TIGROU\ngot      : ");
  who_am_i(5);
  print("\n");

  print("Expected : TIGER\ngot      : ");
  who_am_i(42)
end
